{
    "id": "com.athom.hello",
    "version": "1.0.0",
    "compatibility": ">=1.2.3",
    "name": {
        "en": "Hello World!",
        "nl": "Hallo wereld!"
    },
    "description": {
        "en": "This is an example app for Homey",
